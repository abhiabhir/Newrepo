pipeline {
  agent {
   label 'node3'
  }
  stages {
    stage ('BUILD') {
      steps {
        echo "This is BUILD Stage"
        sh 'sleep 5'
      }
    }
    stage ('TEST') {
      steps {
        echo "This is TEST Stage"
        sh 'sleep 5'
      }
    }
    stage ('DEPLOY') {
      steps {
        echo "This is DEPLOY Stage"
        sh 'sleep 5'
      }
    }
    stage ('Try') {
      steps {
        echo "Trying this Stage"
        sh 'sleep 5'
      }
    } 
  }  
}  
